
    <div class="main" resize ng-class="{'main-collapsed': isCollapsed, 'collaps': collapsed, 'opacity': loader}">
        <div class="wrappers padding">
            <div class="row">

                <div class="col-sm-12">
                    <h1>{{formLabel.title}}</h1>
                </div>

            </div>
            
            <hr style="margin: 5px 0 20px;">
            
            <div class="module row">
                <div class="col-xs-12">
                    <form name="form.gridEditor" action="{{'/api/admin/' + url}}" method="post" target="_blank" novalidate>
                        <div class="tab-pane">
                            <div class="panel">
                                <div class="col-xss-12">
                                   
                                    <input type="hidden" name="type" value="excel" ng-if="preview">
                                             
                                    <div class="form-group">
                                        <label for="input{{formLabel.column[0].id}}">{{formLabel.column[0].name}}</label>
                                        <input type="text" class="btn-groups-filter" name="type" ng-model="catFilter" ng-focus="catFilter = ''" autocomplete="off" placeholder="Filter...">
                                        <span ng-click="clearAllCategory()" class="btn btn-xs btn-default pull-right" ng-show="checkCat">Clear</span>
                                        <div class="btn-groups block" ng-init="form.data.micaId = []">
                                            <label class="btn btn-default col-xs-12 col-sm-6 col-md-4 text-left" ng-repeat="n in category | filter: catFilter" ng-class="{'btn-success': form.data.micaId.indexOf(n.micaId) > -1}" ng-click="catFilter = ''">
                                                <i class="glyphicon glyphicon-ok-sign" ng-show="form.data.micaId.indexOf(n.micaId) > -1"></i>
                                                <i class="glyphicon glyphicon-minus-sign" ng-show="form.data.micaId.indexOf(n.micaId) == -1"></i>
                                                <input type="checkbox" value="{{n.micaId}}" ng-checked="form.data.micaId.indexOf(n.micaId) > -1" ng-click="toggleCategory(n.micaId)" name="{{formLabel.column[0].id}}[]" id="input{{formLabel.column[0].id}}{{n.micaId }}" ng-disabled="disableCat && form.data.micaId.indexOf(n.micaId) == -1">
                                                <label for="input{{formLabel.column[0].id}}{{n.micaId }}">{{n.micaCodeDescription}}</label>
                                            </label>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="clearfix"></div>

                                    </div>

                                    <div class="form-group">
                                        <label for="input{{formLabel.column[1].id}}">{{formLabel.column[1].name}}</label>
                                        <input type="text" class="btn-groups-filter" name="type" ng-model="suppFilter" ng-focus="suppFilter = ''" autocomplete="off" placeholder="Filter...">
                                        <span ng-click="checkAllSupplier()" class="btn btn-xs btn-default pull-right" ng-hide="checkSupp || suppFilter">Check all</span>
                                        <span ng-click="clearAllSupplier()" class="btn btn-xs btn-default pull-right" ng-show="checkSupp">Clear</span>
                                        <div class="btn-groups block" ng-init="form.data.msupId = []">
                                            <label class="btn btn-default col-xs-12 col-sm-6 col-md-4 text-left" ng-repeat="n in supplier | filter: suppFilter" ng-class="{'btn-success': form.data.msupId.indexOf(n.msupId) > -1}" ng-click="$parent.suppFilter = ''">
                                                <i class="glyphicon glyphicon-ok-sign" ng-show="form.data.msupId.indexOf(n.msupId) > -1"></i>
                                                <i class="glyphicon glyphicon-minus-sign" ng-show="form.data.msupId.indexOf(n.msupId) == -1"></i>
                                                <input type="checkbox" value="{{n.msupId}}" ng-checked="form.data.msupId.indexOf(n.msupId) > -1" ng-click="toggleSupplier(n.msupId)" name="{{formLabel.column[1].id}}[]" id="input{{formLabel.column[1].id}}{{n.msupId}}">
                                                <label for="input{{formLabel.column[1].id}}{{n.msupId}}">{{n.msupCodeName}}</label>
                                            </label>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="clearfix"></div>

                                    </div>

                                </div>
                            </div>
                        </div>


                        <div class="button-container">
                            <button type="submit" class="btn btn-primary" ng-disabled="form.data.micaId.length <= 0">View</button>
                            <button type="submit" class="btn btn-success" ng-disabled="form.data.micaId.length <= 0" ng-click="preview = true">Export</button>
               
<!--                            <button type="reset" class="btn btn-danger pull-right" id="reset-button" ng-click="reset()">Clear</button>-->
                        </div>

                        <div class="clearfix"></div>
                    </form>
                   
<!--                    <iframe id="report" name="report" src="{{'/api/admin/' + url}}" style="width: 100%; height: 500px;" ng-if="preview"></iframe>-->
                </div>
            </div>
        </div>
    </div>
    
    <div class="footer">

        <news></news>

        <footer></footer>

    </div>