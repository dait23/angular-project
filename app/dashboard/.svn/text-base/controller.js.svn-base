'use strict';

App.controller('dashboardCtrl', ['$scope', '$location', '$http', '$rootScope', 'api', 'apiGet', '$state', '$timeout', 'isMenu', 'menuContent', function($scope, $location, $http, $rootScope, api, apiGet, $state, $timeout, isMenu, menuContent) {
    
    $scope.collapsed = isMenu.get();
    $scope.menu = menuContent.get();
    
	$scope.htmlReady();
}]);